package th.co.gosoft.audit.cpram.dto;

import java.util.List;

public class ChecklistTypeDTO extends StandardAttributeDTO {
	private int checklistTypeId;
	private String checklistTypeName;	
	private List<ChecklistDTO> checklistId;
	private List<QuestionTypeDTO> questionTypeId;
	private List<GradeDTO> gradeId;
	
	public int getChecklistTypeId() {
		return checklistTypeId;
	}
	public void setChecklistTypeId(int checklistTypeId) {
		this.checklistTypeId = checklistTypeId;
	}
	public String getChecklistTypeName() {
		return checklistTypeName;
	}
	public void setChecklistTypeName(String checklistTypeName) {
		this.checklistTypeName = checklistTypeName;
	}
	public List<ChecklistDTO> getChecklistId() {
		return checklistId;
	}
	public void setChecklistId(List<ChecklistDTO> checklistId) {
		this.checklistId = checklistId;
	}
	public List<QuestionTypeDTO> getQuestionTypeId() {
		return questionTypeId;
	}
	public void setQuestionTypeId(List<QuestionTypeDTO> questionTypeId) {
		this.questionTypeId = questionTypeId;
	}
	public List<GradeDTO> getGradeId() {
		return gradeId;
	}
	public void setGradeId(List<GradeDTO> gradeId) {
		this.gradeId = gradeId;
	}
	
}
