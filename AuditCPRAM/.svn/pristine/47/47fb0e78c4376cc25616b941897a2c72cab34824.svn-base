package th.co.gosoft.audit.cpram.dto;

import java.sql.Date;
import java.sql.Time;

public class CarDTO extends StandardAttributeDTO {
	private int carId;
	private String carNo;
	private ChecklistPlanDTO checklistPlanId; 
	private Date dueDate;
	private Time dueTime;
	private CarStatusDTO carStatusId;
	
	public int getCarId() {
		return carId;
	}
	public String getCarNo() {
		return carNo;
	}
	public ChecklistPlanDTO getChecklistPlanId() {
		return checklistPlanId;
	}
	public Date getDueDate() {
		return dueDate;
	}
	public Time getDueTime() {
		return dueTime;
	}
	public CarStatusDTO getCarStatusId() {
		return carStatusId;
	}
	public void setCarId(int carId) {
		this.carId = carId;
	}
	public void setCarNo(String carNo) {
		this.carNo = carNo;
	}
	public void setChecklistPlanId(ChecklistPlanDTO checklistPlanId) {
		this.checklistPlanId = checklistPlanId;
	}
	public void setDueDate(Date dueDate) {
		this.dueDate = dueDate;
	}
	public void setDueTime(Time dueTime) {
		this.dueTime = dueTime;
	}
	public void setCarStatusId(CarStatusDTO carStatusId) {
		this.carStatusId = carStatusId;
	}
	
}
